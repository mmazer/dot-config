alias bye=exit
alias cd..='cd ..'
alias cfg='git --git-dir=$HOME/.cfg --work-tree=$HOME'
alias cls='clear'
alias clh='history -cw'
alias cpdir='cp -R'
alias ddir='rm -rf'
alias del='rm -f'
alias dused='df -kHal'
alias ed='$EDITOR'
alias findcvs='find . -type d -name CVS && find . -name .cvsignore'
alias findsvn='find . -type d -name .svn'
alias focus='if [ -f ~/.focus ]; then echo -e $(cat ~/.focus); else echo "No focus?"; fi'
alias fn='find . -iname'
alias g='git'
alias gd='git diff'
alias gh='git hist'
alias gr='cd `git rev-parse --git-dir`; cd ..'
alias gs='git status'
alias h='history'
alias la='ls -a'
alias ll='ls -la'
alias lstat='echo $?'
alias lsz='ls -laS'
alias lt='ls -ltr'
alias lu='ls -ltur'
alias lr='ls -lR'
alias lp='ls -al | $PAGER'
alias mark='bookmark'
alias marks='bookmarksshow'
alias mkdir='mkdir -p'
alias mkexe='chmod 755'
alias mkrd='chmod 644'
alias pg='$PAGER'
alias pp='python -m json.tool'
alias py='python'
alias rebash='source ~/.bashrc'
alias real='source ~/.config/bash/aliases'
alias rm='rm -i'
alias rmbak='rm -iv *.bak'
alias rmsvn='find . -type d -name .svn | xargs rm -rv'
alias sterm='echo $TERM'
alias spath='echo $PATH'
alias ssh-hosts='grep -w Host ~/.ssh/config'
alias th='history | tail -12'
alias vi='vim -u ~/.vim/minimal.vim'
alias wf='tail -f'

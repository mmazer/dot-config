# docker
alias dc='docker-compose'
alias dm='docker-machine'
alias docker-alias='alias | grep docker'
alias drmi='docker rmi -f $(docker images -q)'
alias dgrep='docker ps | grep'
alias dprune='docker system prune --all --volumes'
alias ddf='docker system df'
alias denv='eval $(docker-machine env default)'
alias dunset='unset ${!DOCKER_*}'
alias dexec='docker exec -it'
alias dstop='docker stop $(docker ps -a -q)'
alias dkillall='docker kill $(docker ps -q)'
alias dpause='docker pause $(docker ps -q)'
alias dps='docker ps -a'
alias drm='docker rm $(docker ps -a -q)'
alias drun='docker run -it'
alias dbash='docker run -it --entrypoint=bash'
alias dips='for d in $(docker ps -q); do echo -n "$d=>"; docker-ip $d; done'
alias env-docker='env | grep DOCKER'
